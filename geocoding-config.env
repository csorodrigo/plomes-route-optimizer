# Configuration optimized for 2,244 customers processing

# PERFORMANCE TUNING
# Based on analysis: 99% have CEP, majority in CearÃ¡ (CE)
GEOCODING_CONCURRENCY=8
GEOCODING_BATCH_SIZE=50
GEOCODING_DELAY_MS=200
GEOCODING_BATCH_DELAY_MS=2000

# RETRY CONFIGURATION
# Conservative retry settings to handle API limits
GEOCODING_MAX_RETRIES=3
GEOCODING_RETRY_BASE_DELAY=1000
GEOCODING_RETRY_BACKOFF=2

# SAVE FREQUENCY
# Save progress frequently to prevent data loss
GEOCODING_SAVE_INTERVAL=25

# PROVIDER STRATEGY
# Prioritize Brazilian providers for better accuracy
GEOCODING_PREFER_BRAZILIAN=true
GEOCODING_FALLBACK_GLOBAL=true

# REGIONAL OPTIMIZATION
# Optimize for Northeast Brazil (CearÃ¡ focus)
GEOCODING_REGION_PRIORITY=CE,RN,PB,PE,MA,PI

# API QUOTAS (configure based on your available quotas)
# Google Maps: 40,000 requests/month free, then $5/1000 requests
# Mapbox: 100,000 requests/month free, then $0.75/1000 requests
# PositionStack: 25,000 requests/month free

# Uncomment and set your API keys:
# GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
# MAPBOX_API_KEY=your_mapbox_api_key_here
# POSITIONSTACK_API_KEY=your_positionstack_api_key_here

# MONITORING
GEOCODING_ENABLE_METRICS=true
GEOCODING_LOG_LEVEL=info
GEOCODING_PROGRESS_INTERVAL=10

# CACHE SETTINGS
GEOCODING_CACHE_TTL=2592000
GEOCODING_ENABLE_CACHE=true

